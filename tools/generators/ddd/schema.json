{
  "$schema": "http://json-schema.org/schema",
  "cli": "nx",
  "$id": "ddd",
  "title": "nestjs ddd - feature",
  "description": "Create ddd structure for a feature",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "description": "Module name",
      "$default": {
        "$source": "argv",
        "index": 0
      }
    },
    "project": {
      "type": "string",
      "description": "Project",
      "$default": {
        "$source": "argv",
        "index": 1
      }
    }
  },
  "required": ["name", "project"]
}
